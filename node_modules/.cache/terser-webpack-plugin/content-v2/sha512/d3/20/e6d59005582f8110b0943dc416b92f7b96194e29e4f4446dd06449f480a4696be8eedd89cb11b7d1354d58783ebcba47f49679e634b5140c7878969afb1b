\n        setError(false);\n        setConsultar(true)\n      \n\n    }\n    return (\n        <form onSubmit={enviarFormulario}>\n        {error && <Error descripcion=\"Todos los campos son obligatorios\"/>}\n            <div className=\"form-group\">\n                <label htmlFor=\"ciudad\">Ciudad:</label>\n                <input \n                    type=\"text\" \n                    id=\"ciudad\"\n                    name=\"ciudad\"\n                    placeholder=\"Escriba su ciudad...\"\n                    className=\"form-control\"\n                    value={ciudad}\n                    onChange={handleInput}\n                />\n            </div>\n            <div className=\"form-group\">\n            <label htmlFor=\"pais\">País</label>\n                <select \n                    className=\"custom-select\"\n                    id=\"pais\"    \n                    name=\"pais\"\n                    value={pais}\n                    onChange={handleInput}\n                >\n                    <option value=\"\">--Seleccione un pais--</option>\n                    <option value=\"US\">Estados Unidos</option>\n                    <option value=\"MX\">México</option>\n                    <option value=\"AR\">Argentina</option>\n                    <option value=\"CO\">Colombia</option>\n                    <option value=\"CR\">Costa Rica</option>\n                    <option value=\"ES\">España</option>\n                    <option value=\"PE\">Perú</option>\n                    <option value=\"BO\">Bolivia</option>       \n                </select>\n            </div>\n\n            <input type=\"submit\" className=\"btn btn-outline-dark btn-lg btn-block\" value=\"Enviar\"/>\n        </form>\n    )\n}\n\nexport default Formulario\n","import React from \"react\";\nimport '../styles/Header.css'\nfunction Header({ titulo }) {\n  return (\n    <>\n      <header>\n        <div className=\"container \">\n          <nav className=\"navbar navbar-expand-lg navbar-dark bg-dark\">\n            {/* <a className=\"navbar-brand text-size\" href=\"#!\" >\n              {titulo}\n            </a> */\n            }\n            <p className=\"text-center text-white titulo-header text-size\">\n              {titulo}\n            </p>\n          </nav>\n        </div>\n      </header>\n    </>\n  );\n}\n\nexport default Header;\n","import React from \"react\";\nimport '../styles/Clima.css'\nfunction Clima({ data }) {\n  const { name, main } = data;\n  if (!name) return null;\n  const kelvin = 273.15;\n  let _temperatura_actual = parseFloat(main.temp - kelvin,10).toFixed(2);\n  let _temperatura_minima = parseFloat(main.temp_min - kelvin,10).toFixed(2);\n  let _temperatura_maxima = parseFloat(main.temp_max - kelvin,10).toFixed(2);\n  return (\n    <>\n    <h2 class=\"mt-4 text-midle\">Datos del Clima</h2>\n    <div className=\"card mt-3\">\n      <h3 className=\"card-header size-title\">La temperatura en <span className=\"font-weight-bold\">{name}</span> es:</h3>\n      <div className=\"card-body\">\n        <h4 className=\"card-title temperatura text-center tempe\">{_temperatura_actual} C&deg;</h4>\n        \n          <ul>\n              <li className=\"text-center temp-min-max size-min-max\">La mínima: {_temperatura_minima} C&deg; </li>\n              <li className=\"text-center temp-min-max size-min-max\">La máxima: {_temperatura_maxima} C&deg;</li>\n          </ul>\n        \n        <div className=\"card-footer text-center text-info humedad size-min-max\"> Humedad Relativa: {main.humidity} %</div>\n      </div>\n    </div>\n    </>\n  );\n}\n\nexport default Clima;\n","\nimport React, {Fragment, useState, useEffect} from 'react'\nimport Formulario from './components/Formulario'\nimport Header from './components/Header'\nimport Clima from './components/Clima'\nimport Error from './components/Error'\n\nimport './styles/App.css'\nfunction App() {\n  const [busqueda, setBusqueda] = useState({\n    ciudad:'',\n    pais:'',\n  });\n  const {ciudad,pais} = busqueda;\n\n  const [consultar,setConsultar] = useState(false);\n  const [data, setData] = useState({}); //almacena la respuesta de la api en json\n  const [error, setError] = useState(false);\n\n  useEffect(()=> {\n    const consultarAPI = async() => {\n      if(consultar){\n        let keyAPI = `eaed9e10df601aab920b0f2f1e13df89`;\n        let urlAPI = `https://api.openweathermap.org/data/2.5/weather?q=${ciudad},${pais}&appid=${keyAPI}`;\n        let response = await fetch(urlAPI);\n        let data = await response.json(); //data tiene la respuesta de la api en json\n        setData(data);\n        setConsultar(false);\n\n        //verificar si la ciudad introducida es valida\n        if(data.cod === '404')\n          setError(true);\n        else\n          setError(false);  \n        \n        console.log(data)\n\n        //verificando si hay o no resultados en la consulta\n        if(data.cod === \"404\"){\n          setError(true);\n        }else{\n          setError(false);\n        }\n      }\n    };\n    consultarAPI();\n  },[consultar]);\n\n  let componente;\n  if(error) {\n    componente = <Error descripcion = {data.message}/> ;\n  }\n  else{\n    componente = <Clima data={data}/>\n  }\n  return (\n    <>\n    <Header titulo=\"Aplicación de Clima con React\"/>\n    <div className=\"container\">\n      <div className=\"row\">\n        <div className=\"col-md-6 col-sm-12 mt-3\">\n          <h2 className=\"size-title\">Formulario de consulta</h2>\n          <Formulario \n            busqueda={busqueda}\n            setBusqueda={setBusqueda}\n            setConsultar={setConsultar}\n          />\n        </div>\n        <div className=\"col-md-6 col-sm-12\">\n          \n          {componente}\n        </div>\n      </div>\n    </div>\n    </>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\nimport App from './App';\nimport 'bootstrap/dist/css/bootstrap.min